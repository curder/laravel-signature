{
  "name": "hypocenter/laravel-signature",
  "type": "library",
  "description": "Signature helper for Laravel",
  "authors": [
    {
      "name": "hypo",
      "email": "hypomail@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/hypocenter/laravel-signature/issues",
    "source": "https://github.com/hypocenter/laravel-signature"
  },
  "autoload": {
    "psr-4": {
      "Hypocenter\\LaravelSignature\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hypocenter\\LaravelSignature\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2",
    "laravel/framework": "^5.5||^6.0||^7.0",
    "ext-json": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hypocenter\\LaravelSignature\\SignatureServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.8",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.8|^4.4|^5.0"
  }
}
